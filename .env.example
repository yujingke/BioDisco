# BioDisco Environment Configuration
# Copy this file to .env and fill in your actual values

# OpenAI API Configuration (Required)
OPENAI_API_KEY_OPENAI=your_openai_api_key_here
OPENAI_API_BASE_OPENAI=https://api.openai.com/v1

# Free API Configuration (Optional - for free API usage)
OPENAI_API_KEY_FREE=your_free_api_key_here
OPENAI_API_BASE_FREE=https://api.llm7.io/v1
# Neo4j Database Configuration (Optional - for knowledge graph functionality)
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_neo4j_password
KG_PATH=/path/to/your/kg_specific_files

# Lit and KG integration (Optional)#
DISABLE_PUBMED=False
DISABLE_KG=False




